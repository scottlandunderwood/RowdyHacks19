<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="490.0" prefWidth="482.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.MainController">
   <children>
      <AnchorPane layoutX="-179.0" layoutY="-125.0" prefHeight="325.0" prefWidth="379.0">
         <children>
            <TextField fx:id="noteName" layoutX="212.0" layoutY="219.0" prefHeight="25.0" prefWidth="183.0" promptText="Title of note">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </TextField>
            <ImageView fx:id="goBack" fitHeight="45.0" fitWidth="47.0" layoutX="180.0" layoutY="125.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../Data/img/Note.jpg" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
      <TextArea fx:id="note" layoutX="32.0" layoutY="127.0" prefHeight="292.0" prefWidth="431.0" promptText="Enter note here..." />
      <Button fx:id="addNote" layoutX="416.0" layoutY="424.0" mnemonicParsing="false" onAction="#SaveNote" prefHeight="45.0" prefWidth="47.0" style="-fx-background-radius: 70;" text="+">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
